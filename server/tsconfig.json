{
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.cache/tsbuildinfo",
    "target": "ES2020",
    "module": "commonjs",
    "rootDir": ".",
    "outDir": "dist",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "incremental": false
  },
  "include": ["./src", "prisma"],
  "exclude": ["node_modules", "dist"]
}
