{
  "name": "thinklabweb",
  "version": "1.0.0",
  "description": "",
  "main": "server/dist/server.js",
  "type": "module",
  "scripts": {
    "build:server": "tsc --project server/tsconfig.json",
    "build": "vite build",
    "start": "npm run build && node server/dist/server.js",
    "server": "nodemon --watch server --ext ts --exec \"ts-node --files server/server.ts\"",
    "client": "vite",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "preview": "vite preview",
    "db:setup": "npx prisma db push && npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset",
    "db:seed": "node prisma/seed.ts",
    "format": "prettier --write \"client/src/**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"client/src/**/*.{ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "ThinkLab",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "@tailwindcss/vite": "^4.1.17",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "celebrate": "^15.0.3",
    "concurrently": "^9.1.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "formik": "^2.4.6",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "prisma": "^6.2.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1",
    "react-toastify": "^11.0.3",
    "strong-error-handler": "^5.0.15",
    "tailwindcss": "^4.1.17",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.12.12",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^4.2.1",
    "nodemon": "^3.1.9",
    "prettier": "^3.2.5",
    "terser": "^5.37.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^6.0.7"
  }
}
