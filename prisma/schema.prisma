datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
    previewFeatures = ["postgresqlExtensions"]
}

model User {
    id Int @id @default(autoincrement())
    name String
    email String @unique
    password String
    createAt DateTime @default(now())

    //RelationShip
}
